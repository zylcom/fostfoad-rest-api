{
  "author": "sabilillah <sabilillah272@gmail.com>",
  "name": "fostfoad-rest-api",
  "version": "1.0.0",
  "description": "RESTful API",
  "main": "api/index.ts",
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "tsx watch api/index.ts",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "postinstall": "prisma generate",
    "db:reset": "prisma migrate reset -f --skip-seed",
    "test": "jest -iv",
    "format": "prettier -w .",
    "prepare": "husky",
    "start": "node api/index.js"
  },
  "keywords": [
    "hono",
    "nodejs",
    "prisma"
  ],
  "dependencies": {
    "@hono/node-server": "^1.12.1",
    "@hono/zod-validator": "^0.2.2",
    "@prisma/client": "^5.18.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "hono": "^4.5.8",
    "libphonenumber-js": "^1.11.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.7.1",
    "typescript": "^5.5.4"
  }
}
